{
  "manifest_version": 3,
  "name": "Laych Web Tools",
  "description": "A collection of web tools to craft quality software.",
  "version": "1.0",
  "action": {
  },
  "background": {
    "service_worker": "./serviceWorker.js",
    "type": "module"
  },
  "permissions": [
    "scripting",
    "activeTab",
    "tabs"
  ],
  "host_permissions": [
    "http://*/",
    "https://*/"
  ]
}
